action a: 1;
action b: 1;
action c: 1;
action d: 2;

procedure test_undefined_pick_var() {
    pick #X {
        a(#X);
        b(#Y); // ERROR unresolved variable `Y`
    }

    pick #X, #Y {
        pick #A {
            a(#X);
            b(#A);
            c(#Z); // ERROR unresolved variable `Z`
        }
    }

    pick #S {
        d(#S, #T); // ERROR unresolved variable `T`
    }
}
