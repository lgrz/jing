action up: 0;
action down: 0;
action left: 0;
action right: 0;

procedure test_search() {
    // empty search block
    search {
    }

    // search with action call
    search {
        up;
    }

    // search with multiple action calls
    search {
        up;
        down;
        left;
        right;
    }
}
