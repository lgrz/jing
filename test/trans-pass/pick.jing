// EXPECTED OUTPUT
// proc(test_pick, [pi(X, pi(Y, [pi(A, pi(B, [a(X), b(A), c]))])), pi(A, [c, pi(B, [d(B), e])])]).

action a: 1;
action b: 1;
action c: 0;
action d: 1;
action e: 0;

procedure test_pick() {
    pick #X, #Y {
        pick #A, #B {
           a(#X);
           b(#A);
           c;
        }
    }
    pick #A {
        c;
        pick #B {
            d(#B);
            e;
        }
    }
}
