// EXPECTED OUTPUT
// proc(test_pick, [pi(x, pi(y, [pi(a, pi(b, [a(x), b(a), c]))])), pi(a, [c, pi(b, [d(b), e])])]).

action a: 1;
action b: 1;
action c: 0;
action d: 1;
action e: 0;

procedure test_pick() {
    pick #x, #y {
        pick #a, #b {
           a(#x);
           b(#a);
           c;
        }
    }
    pick #a {
        c;
        pick #b {
            d(#b);
            e;
        }
    }
}
