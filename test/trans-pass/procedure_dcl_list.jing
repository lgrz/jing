action up: 0;
action down: 0;
action left: 0;
action right: 0;

procedure go_up() {
    up;
}

procedure go_down() {
    down;
}

procedure go_left() {
    left;
}

procedure go_right() {
    right;
}

procedure full_circle() {
    up;
    down;
    left;
    right;
}
