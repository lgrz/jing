action up: 0;
action down: 0;
action left: 0;
action right: 0;
action three: 0;
action four: 0;
action five: 0;
action six: 0;
action seven: 0;
action eight: 0;

procedure go_up() {
    ndet {
        eat: {
            up;
            down;
        }
        drink: {
            left;
            right;
        }
        sleep: {
            three;
            four;
        }
        die: {
            five;
            six;
        }
        john: {
            seven;
            eight;
        }
    }
    conc {
        sleep: {
            three;
            four;
        }
        die: {
            five;
            six;
        }
        john: {
            seven;
            eight;
        }
    }
    pconc {
        eat: {
            up;
            down;
        }
        drink: {
            left;
            right;
        }
    }
}
